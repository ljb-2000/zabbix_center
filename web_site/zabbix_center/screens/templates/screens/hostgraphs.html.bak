{% extends 'screens/base.html' %} {% block title %}{{ graphs.0.host }}{% endblock %} {% block content %}
<div class="container bs-docs-container">
<div class="row">
	<div class="col-md-3">
        <div class="bs-sidebar hidden-print" role="complementary">
          	<ul class="nav bs-sidenav">
            {% for graph in graphs %}
            <li>
            <a href='#{{ graph.name }}'>{{ graph.name }}</a>
            <ul class="nav">
                	{% for item in graph.content %}
                	<li>
  					<a href="#{{ item.graphid }}">{{ item.name }}</a>
  					</li>
  					{% endfor %}		
            </ul>
            </li>
            {% endfor %}
    		</ul>
    	</div>
   	</div>
	<div class="col-md-9" role="main">
		<div class="bs-docs-section">
			{% for graph in graphs %}
				<h3 id='{{ graph.name }}'>{{ graph.name }}</h3>
				{% for item in graph.content %}
				<h5 id='{{ item.graphid }}'>{{ item.name }}</h5>
				<li>
				<img src="http://192.168.1.203:82/chart2.php?period=3600&graphid={{ item.graphid }}&width=700" alt='' /> 
				</li>
				{% endfor %}
			{% endfor %}
		</div>
	</div>
</div>
</div>


{% endblock %}